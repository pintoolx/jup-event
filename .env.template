# Privy Configuration
# Get your Privy App ID from https://dashboard.privy.io/
VITE_PRIVY_APP_ID=your_privy_app_id_here

# Supabase Configuration
# Get these from https://supabase.com/dashboard/project/_/settings/api
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# Solana RPC Configuration
# Recommended: Use Helius, QuickNode, or similar high-performance RPC
VITE_RPC_URL=https://api.mainnet-beta.solana.com

# Jupiter API Key (optional but recommended for better rate limits)
VITE_JUPITER_API_KEY=your_jupiter_api_key_here

# Debug Mode (set to 'true' for testing with 1 JUP instead of 250 JUP)
VITE_DEBUG_MODE=false

# Atomic Swap Configuration
# Target wallet address to receive JUP tokens after swap
VITE_TARGET_ADDRESS=your_target_wallet_address_here
